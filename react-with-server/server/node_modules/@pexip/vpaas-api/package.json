{
  "name": "@pexip/vpaas-api",
  "version": "18.0.2",
  "description": "VPaaS API Wrapper",
  "homepage": "https://gitlab.com/pexip/zoo",
  "bugs": "https://gitlab.com/pexip/zoo/issues",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/pexip/zoo.git",
    "directory": "src/aquila/packages/vpaas-api"
  },
  "license": "Apache-2.0",
  "author": "Pexip",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./*.js": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pexip-bundler build",
    "clean": "rm -fr dist",
    "prepack": "yarn clean && yarn build --no-sourcemap",
    "generate-api": "api-generator ws",
    "test": "pexip-bundler test --passWithNoTests",
    "check-format": "prettier --ignore-path=../../.prettierignore --check .",
    "format": "prettier --ignore-path=../../.prettierignore --write .",
    "lint": "yarn run -T eslint --ext .js,.ts,.tsx --report-unused-disable-directives .",
    "typecheck": "yarn tsc --noEmit -p tsconfig.build.json"
  },
  "devDependencies": {
    "@pexip/api-generator": "16.5.1",
    "@pexip/bundler": "16.6.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}