{
  "name": "@pexip/media-processor",
  "version": "18.3.0",
  "description": "Media processor for video and audio",
  "homepage": "https://gitlab.com/pexip/zoo",
  "bugs": "https://gitlab.com/pexip/zoo/issues",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/pexip/zoo.git",
    "directory": "src/aquila/packages/media-processor"
  },
  "license": "Apache-2.0",
  "module": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:main && yarn build:worklets && yarn build:workers",
    "build:denoise": "cd denoise && wasm-pack build -t web && printf '%s\n%s\n' \"import {TextDecoder, TextEncoder} from './EncoderDecoder';\" \"$(cat ./pkg/denoise.js)\" >./pkg/denoise.js && prettier --write pkg && cp pkg/denoise* ../../denoise/",
    "build:main": "yarn pexip-bundler build --bundler=tsc -c ./src/main",
    "build:workers": "pexip-bundler build --bundler=esbuild -o dist/workers -b src/workers/*.ts",
    "build:worklets": "pexip-bundler build --bundler=esbuild -o dist/worklets -b src/worklets/*.ts",
    "check-format": "prettier --ignore-path=../../.prettierignore --check .",
    "clean": "rm -fr dist",
    "format": "prettier --ignore-path=../../.prettierignore --write .",
    "lint": "yarn run -T eslint --ext .js,.ts,.tsx --report-unused-disable-directives .",
    "prepack": "yarn clean && yarn build:main --no-sourcemap && yarn build:worklets --no-sourcemap && yarn build:workers --no-sourcemap",
    "test": "pexip-bundler test",
    "tsc": "tsc --noEmit",
    "typecheck": "yarn tsc --noEmit -p .",
    "update-assets": "yarn run update-tasks-vision-assets",
    "update-deeplabv3-model": "curl --output deeplabv3.tflite -L --output-dir ../media-assets/@mediapipe/models/ --create-dirs 'https://tfhub.dev/tensorflow/lite-model/deeplabv3/1/metadata/2?lite-format=tflite'",
    "update-media-assets": "yarn update-tasks-vision-assets && yarn update-models",
    "update-models": "yarn update-deeplabv3-model && yarn update-selfie-models",
    "update-selfie-landscape-model": "curl -OL --output-dir ../media-assets/@mediapipe/models/ --create-dirs https://storage.googleapis.com/mediapipe-models/image_segmenter/selfie_segmenter_landscape/float16/latest/selfie_segmenter_landscape.tflite",
    "update-selfie-models": "yarn update-selfie-square-model && yarn update-selfie-landscape-model",
    "update-selfie-square-model": "curl -OL --output-dir ../media-assets/@mediapipe/models/ --create-dirs https://storage.googleapis.com/mediapipe-models/image_segmenter/selfie_segmenter/float16/latest/selfie_segmenter.tflite",
    "update-tasks-vision-assets": "unzip -jo ../../.yarn/cache/@mediapipe-tasks-vision-npm-* \"*/wasm/*\" \"*.json\" -d ../media-assets/@mediapipe/tasks-vision/wasm/"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.14",
    "@pexip/denoise": "16.4.0",
    "@pexip/utils": "16.12.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@pexip/bundler": "16.6.1",
    "@types/audioworklet": "^0.0.41",
    "@types/dom-webcodecs": "^0.1.4",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/trusted-types": "^2.0.2",
    "prettier": "^3.2.5",
    "typescript": "~5.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}