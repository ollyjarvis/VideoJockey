    "@react-stately/collections": "^3.10.5",
    "@react-stately/menu": "^3.6.1",
    "@react-stately/overlays": "^3.6.5",
    "@react-stately/tooltip": "^3.4.7",
    "@react-stately/tree": "^3.7.6",
    "change-case": "^4.1.1",
    "classnames": "^2.2.6",
    "focus-visible": "^5.1.0",
    "normalize.css": "^8.0.1",
    "simplebar-core": "^1.2.3",
    "simplebar-react": "^3.2.3",
    "tabbable": "^6.0.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@pexip/bundler": "16.6.1",
    "@react-types/button": "^3.9.2",
    "@react-types/menu": "^3.9.7",
    "@react-types/shared": "^3.22.1",
    "@storybook/addon-actions": "^7.6.15",
    "@storybook/addon-docs": "^7.6.15",
    "@storybook/blocks": "^7.6.15",
    "@storybook/react": "^7.6.15",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/classnames": "^2.2.10",
    "@types/csso": "^5.0.0",
    "@types/jest-axe": "^3.5.0",
    "@types/node-fetch": "^2.5.8",
    "@types/prettier": "^2.0.2",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/testing-library__react-hooks": "^3.4.1",
    "@types/testing-library__user-event": "^4.2.0",
    "@types/uuid": "^8.3.0",
    "builtin-modules": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-axe": "^7.0.0",
    "node-fetch": "^3.0.0",
