{
  "name": "@pexip/components",
  "version": "16.15.1",
  "homepage": "https://gitlab.com/pexip/zoo",
  "bugs": "https://gitlab.com/pexip/zoo/issues",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/pexip/zoo.git",
    "directory": "src/aquila/packages/components"
  },
  "license": "Apache-2.0",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "main": "./dist/components.js",
  "module": "./dist/components.mjs",
  "types": "./dist/types/src/index.d.ts",
  "files": [
    "/dist",
    "/design-tokens",
    "/src/fonts.css",
    "/src/assets/fonts/*"
  ],
  "scripts": {
    "build": "pexip-bundler build --bundler vite && yarn typescript-declarations",
    "build:no-sourcemap": "pexip-bundler build --bundler vite --no-sourcemap && yarn typescript-declarations",
    "check-format": "prettier --ignore-path=../../.prettierignore --check .",
    "clean": "rm -fr dist/",
    "echo": "echo echo",
    "format": "prettier --ignore-path=../../.prettierignore --write .",
    "lint": "yarn run -T eslint --ext .js,.ts,.tsx --report-unused-disable-directives .",
    "prepack": "yarn clean && yarn build:no-sourcemap",
    "prettier": "prettier --ignore-path ../../.gitignore \"**/*.+(js|json|ts|tsx)\"",
    "test": "pexip-bundler test",
    "tsc": "tsc --noEmit",
    "typecheck": "yarn tsc --noEmit -p .",
    "typescript-declarations": "tsc --project tsconfig.declarations.json"
  },
  "dependencies": {
    "@pexip/hooks": "17.1.2",
    "@pexip/signal": "16.7.0",
    "@pexip/utils": "16.12.0",
