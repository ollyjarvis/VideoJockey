{
  "name": "@pexip/utils",
  "version": "16.12.0",
  "description": "A collection of utility function helpers",
  "homepage": "https://gitlab.com/pexip/zoo",
  "bugs": "https://gitlab.com/pexip/zoo/issues",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/pexip/zoo.git",
    "directory": "src/aquila/packages/utils"
  },
  "license": "Apache-2.0",
  "main": "dist",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "pexip-bundler build",
    "clean": "rm -rf dist/",
    "prepack": "yarn clean && yarn build --no-sourcemap",
    "start": "pexip-bundler dev",
    "test": "pexip-bundler test",
    "tsc": "tsc --noEmit",
    "check-format": "prettier --ignore-path=../../.prettierignore --check .",
    "format": "prettier --ignore-path=../../.prettierignore --write .",
    "lint": "yarn run -T eslint --ext .js,.ts,.tsx --report-unused-disable-directives .",
    "typecheck": "yarn tsc --noEmit -p ."
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@pexip/bundler": "16.6.1",
    "typescript": "~5.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}